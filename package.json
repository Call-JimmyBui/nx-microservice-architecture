{
  "name": "@microservice/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "g-proto-types": "npx protoc --proto_path=./proto --ts_proto_out=./types/proto/ ./proto/*.proto --ts_proto_opt=nestJs=true",
    "g-proto-types:force": "npm run g-proto-types -- --skip-nx-cache",
    "g-proto-types:all": "npx protoc --proto_path=./proto --ts_proto_out=./types/ --ts_proto_opt=nestJs=true ./proto/**/*.proto",
    "g-proto-types:all:force": "npm run g-proto-types:all -- --skip-nx-cache",
    "dev:user": "nx serve user-service",
    "dev:product": "nx serve product-service",
    "dev:order": "nx serve order-service",
    "dev:payment": "nx serve payment-service",
    "dev:cart": "nx serve cart-service",
    "dev:gateway": "nx serve api-gateway",
    "dev:noti": "nx serve notification-service",
    "dev:all": "nx run-many --target=serve --all --parallel"
  },
  "nx": {
    "targets": {
      "g-proto-types": {
        "cache": true,
        "inputs": [
          "{workspaceRoot}/proto/*.proto"
        ]
      }
    }
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@nestjs/common": "^11.0.0",
