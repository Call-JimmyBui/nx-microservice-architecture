    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
    "@nx/jest": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/nest": "21.2.3",
    "@nx/node": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/webpack": "21.2.3",
