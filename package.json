    "@swc/helpers": "~0.5.11",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "29.5.14",
    "@types/node": "~18.16.9",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "7.16.0",
